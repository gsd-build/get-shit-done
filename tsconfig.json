{                                                                                                                 
  "compilerOptions": {                                                                                            
    "target": "ES2020",                                                                                           
    "lib": ["ES2020"],                                                                                            
    "module": "ESNext",                                                                                           
    "moduleResolution": "bundler",                                                                                
    "jsx": "preserve",                                                                                            
    "strict": true,                                                                                               
    "skipLibCheck": true,                                                                                         
    "noEmit": true,                                                                                               
    "types": ["node"]                                                                                             
  },                                                                                                              
  "include": ["tsx/**/*.tsx", "tsx/**/*.ts"]                                                                      
}