{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Get Shit Done",
  "theme": "prism",
  "layout": "sidenav",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "favicon": "/favicon.png",
  "topbar": {
    "style": "gradient"
  },
  "background": {
    "style": "grid"
  },
  "backgroundImage": "/bg.svg",
  "modeToggle": {
    "default": "dark"
  },
  "rounded": "sharp",
  "sidebar": {
    "items": "card"
  },
  "codeBlock": {
    "mode": "dark"
  },
  "colors": {
    "primary": "#68ACD7",
    "light": "#7FA1D1",
    "dark": "#68ACD7",
    "background": {
      "light": "#FFFFFF",
      "dark": "#1A1B26"
    },
    "anchors": {
      "from": "#68ACD7",
      "to": "#629457"
    }
  },
  "font": {
    "headings": {
      "family": "Hubot Sans",
      "url": "https://mintlify-assets.b-cdn.net/fonts/Hubot-Sans.woff2",
      "format": "woff2"
    },
    "body": {
      "family": "Hubot Sans",
      "url": "https://mintlify-assets.b-cdn.net/fonts/Hubot-Sans.woff2",
      "format": "woff2"
    }
  },
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "installation"]
    },
    {
      "group": "Core Concepts",
      "pages": ["workflow", "why-it-works"]
    },
    {
      "group": "Reference",
      "pages": ["commands", "configuration"]
    }
  ],
  "footer": {
    "socials": {
      "github": "https://github.com/glittercowboy/get-shit-done",
      "discord": "https://discord.gg/5JJgD5svVS"
    }
  }
}
